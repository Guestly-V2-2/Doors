---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()

function GetRoom()
        local gruh = workspace.CurrentRooms
        return gruh:FindFirstChild(game.ReplicatedStorage.GameData.LatestRoom.Value)
end

-- CONFIG
local CHANGE_DELAY = 0.05 -- thời gian đổi mặt (giây)

-- Danh sách Texture ID (bạn tự thay ID vào đây)
local Textures = {
    "rbxassetid://91207064517129",
    "rbxassetid://125879637567921",
    "rbxassetid://101719063393555",
    "rbxassetid://106422062721387",
}

-- PATH
local emitter = workspace
    :WaitForChild("Lightshaker")
    :WaitForChild("greed")
    :WaitForChild("Attachment")
    :WaitForChild("ParticleEmitter")

-- SAFETY CHECK
if not emitter or not emitter:IsA("ParticleEmitter") then
    warn("❌ Không tìm thấy ParticleEmitter")
    return
end

-- LOOP ĐỔI MẶT
task.spawn(function()
    local index = 1
    while true do
        emitter.Texture = Textures[index]

        index += 1
        if index > #Textures then
            index = 1
        end

        task.wait(CHANGE_DELAY)
    end
end)

local model = game.Workspace.Lightshaker
local models = game.Workspace.Lightshaker:FindFirstChildWhichIsA("BasePart") or game.Workspace.Lightshaker:FindFirstChildWhichIsA("Part")
models.CFrame = game.Workspace.CurrentRooms:GetChildren()[#game.Workspace.CurrentRooms:GetChildren() - 1].Parts.Floor.CFrame + Vector3.new(0,8,0)

game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 255, 0)
game.Lighting.MainColorCorrection.Contrast = 0.01
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()
damages = false
model:Destroy()
end)

---====== Run entity ======---

entity:Run()
