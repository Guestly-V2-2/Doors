local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/Voor-Pr00/Fogroom/refs/heads/main/Spawner%20v3"))()

---====== Create entity ======---

local entity = spawner.Create({
	Entity = {
		Name = "Screamer",
		Asset = "rbxassetid://125074363240064",
		HeightOffset = 0
	},
	Lights = {
		Flicker = {
			Enabled = true,
			Duration = 10
		},
		Shatter = true,
		Repair = false
	},
	Light = {
		Color = {255, 1, 1}
	},

	Earthquake = {
		Enabled = false
	},
	CameraShake = {
		Enabled = true,
		Range = 150,
		Values = {3, 5, 7, 5}
	},
	Movement = {
		Speed = 130,
		Delay = 10,
		Reversed = false
	},
	Rebounding = {
		Enabled = true,
		Type = "Ambush",
		Min = 3,
		Max = 3,
		Delay = 2
	},
	Damage = {
		Enabled = true,
		Range = 40,
		Amount = 120
	},
	Crucifixion = {
		Enabled = true,
		Range = 40,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding",
		Hints = {"Are you serious right now??", "How could you die by a rusher entity", "You are so pathetic", "Do you know what is hiding?", "Bet you can't do better next time.."},
		Cause = "Screamer"
	}
})

---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
end)

entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)

entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime == true then
        print("Entity has entered room: ".. room.Name.. " for the first time")
    else
        print("Entity has entered room: ".. room.Name.. " again")
    end
end)

entity:SetCallback("OnLookAt", function(lineOfSight)
	if lineOfSight == true then
		print("Player is looking at entity")
	else
		print("Player view is obstructed by something")
	end
end)

entity:SetCallback("OnRebounding", function(startOfRebound)
    if startOfRebound == true then
        print("Entity has started rebounding")
	else
        print("Entity has finished rebounding")
	end
end)

entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
end)

entity:SetCallback("OnDespawned", function()
    print("Entity has despawned")
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
--// ‚ö†Ô∏è JUMPSCARE SCRIPT by ChatGPT
-- D√†nh cho executor trong Doors Roblox
-- Gi·ªØ t·ªâ l·ªá g·ªëc, gi·ªØa m√†n h√¨nh, c√≥ n·ªÅn t·ªëi + √°nh s√°ng ƒë·ªè nh·∫•p nh√°y khi b·∫Øt ƒë·∫ßu

local faces = { -- üß† Danh s√°ch 5 m·∫∑t th·ª±c th·ªÉ (thay ID h√¨nh c·ªßa b·∫°n v√†o ƒë√¢y)
    "rbxassetid://97841695990301", 
    "rbxassetid://97841695990301",
    "rbxassetid://97841695990301",
    "rbxassetid://97841695990301",
    "rbxassetid://97841695990301"
}

local soundURL = "rbxassetid://3317082923" -- üîä Thay b·∫±ng link GitHub mp3 c·ªßa b·∫°n

--// GUI setup
local player = game.Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = PlayerGui

--// N·ªÅn t·ªëi m·ªù
local darkOverlay = Instance.new("Frame")
darkOverlay.Size = UDim2.new(1, 0, 1, 0)
darkOverlay.BackgroundColor3 = Color3.new(0, 0, 0)
darkOverlay.BackgroundTransparency = 0.5
darkOverlay.Parent = screenGui

--// Flash ƒë·ªè nh·∫•p nh√°y
local redFlash = Instance.new("Frame")
redFlash.Size = UDim2.new(1, 0, 1, 0)
redFlash.BackgroundColor3 = Color3.fromRGB(3, 255, 133) -- üî¥ M√†u ƒë·ªè
redFlash.BackgroundTransparency = 1
redFlash.ZIndex = 10
redFlash.Parent = screenGui

--// ·∫¢nh m·∫∑t th·ª±c th·ªÉ
local faceImage = Instance.new("ImageLabel")
faceImage.Size = UDim2.new(0.6, 0, 0.6, 0)
faceImage.Position = UDim2.new(0.5, 0, 0.5, 0)
faceImage.AnchorPoint = Vector2.new(0.5, 0.5)
faceImage.BackgroundTransparency = 1
faceImage.ImageTransparency = 0
faceImage.ImageColor3 = Color3.new(1, 1, 1)
faceImage.ScaleType = Enum.ScaleType.Fit
faceImage.Parent = screenGui

--// √Çm thanh jumpscare
local sound = Instance.new("Sound", workspace)
sound.SoundId = soundURL
sound.Volume = 1.5
sound.PlaybackSpeed = 0.9
sound:Play()

--// ‚ö° Flash ƒë·ªè nhanh l√∫c b·∫Øt ƒë·∫ßu
local tweenService = game:GetService("TweenService")
local flashIn = tweenService:Create(redFlash, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 0.3
})
local flashOut = tweenService:Create(redFlash, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 1
})

flashIn:Play()
flashIn.Completed:Wait()
flashOut:Play()

--// ƒê·ªïi m·∫∑t trong 2 gi√¢y (0.1 gi√¢y m·ªói l·∫ßn)
local duration = 2
local startTime = tick()
local lastIndex = 1

while tick() - startTime < duration do
    local i = math.random(1, #faces)
    lastIndex = i
    faceImage.Image = faces[i]
    task.wait(0.1)
end

--// M·∫∑t cu·ªëi c√πng ph√≥ng to + fade out trong 3 gi√¢y
faceImage.Image = faces[lastIndex]
local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)

local enlarge = tweenService:Create(faceImage, tweenInfo, {
    Size = UDim2.new(1.5, 0, 1.5, 0),
    ImageTransparency = 1
})
local fadeOverlay = tweenService:Create(darkOverlay, tweenInfo, {
    BackgroundTransparency = 1
})

enlarge:Play()
fadeOverlay:Play()
enlarge.Completed:Wait()

--// X√≥a GUI v√† √¢m thanh sau khi k·∫øt th√∫c
sound:Destroy()
screenGui:Destroy()
	else
		print("Entity has damaged the player")
	end
end)

--[[
DEVELOPER NOTE:
By overwriting 'CrucifixionOverwrite' the default crucifixion callback will be replaced with your custom callback.

entity:SetCallback("CrucifixionOverwrite", function()
    print("Custom crucifixion callback")
end)
]]--

---====== Run entity ======---

entity:Run()
