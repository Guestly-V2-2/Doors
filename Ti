workspace.Players.Killers.Slasher["Left Leg"].Transparency = 0

    local GashingWoundButton = game:GetService("Players").LocalPlayer.PlayerGui.MainUI.AbilityContainer.GashingWound

    GashingWoundButton.MouseButton1Click:Connect(function() 

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "GS"
screenGui.Parent = playerGui
screenGui.IgnoreGuiInset = true

local red = Instance.new("ImageLabel")
red.Name = "GS"
red.Size = UDim2.new(1, 0, 1, 0)
red.BackgroundTransparency = 1 -- Make the background fully transparent
red.Image = "rbxassetid://136024837694162" -- Set the image using a random Image ID
red.ImageTransparency = 1
red.Parent = screenGui
wait(0.1)
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Players.LocalPlayer.PlayerGui.GS.GS, TweenInfo.new(1),{ImageTransparency = 0})
TW:Play()

function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Name = "GS"
    sound.PlaybackSpeed = 1.1
    sound.Volume = 3
   sound:Play()
end

CustomGitSound("https://github.com/Guestly-Second-Account/Scripts/raw/refs/heads/main/YTSave.com_YouTube_Glitch-Sound-Effect_Media_7a5UMZ4kihM_004_360p.mp3?raw=true", 1, " GS")

wait(3)
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Players.LocalPlayer.PlayerGui.GS.GS, TweenInfo.new(2),{ImageTransparency = 1})
TW:Play()
wait(2.1)
workspace.GS:Destroy()
game.Players.LocalPlayer.PlayerGui.GS:Destroy()
end)

