local SlashTime = 12
local SlashBool = false
if workspace:GetAttribute("ServerType") == "VIP" then
    local cooldown = game:GetService("Players").LocalPlayer.PlayerGui.MainUI.ModScreen.ModContainer.Contents:GetChildren()[9].Title
    spawn(function() 
        while wait(0.1) do 
            if cooldown.Text == "Disable Ability Cooldowns (Currently: true)" then
                SlashTime = 0
                SlashBool = false
            else
                SlashTime = 12
            end
        end
    end) 
end
local Con = require(game:GetService("ReplicatedStorage").Assets.Killers.Slasher.Config)

    Con.Animations.Idle = "rbxassetid://94895464960972"
    Con.Animations.Run = "rbxassetid://138660433982140"
    Con.Animations.Slash = "rbxassetid://109667959938617"
    Con.Animations.Behead = "rbxassetid://126681776859538"
