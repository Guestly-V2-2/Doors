local P = Player.Character
game.StarterGui:SetCore("SendNotification", {Title = "PLEASE READ THIS!", Text = "You have to execute the script twice before the round and after the round", Duration = 4,})
if P.parent == workspace.Players.Killers and P.Name == "Slasher" then
    game:GetService("ReplicatedStorage").Assets.Sounds.OST.eternalFight.SoundId = "105135449947797"
    game:GetService("ReplicatedStorage").Assets.Sounds.OST.eternalFight.PlaybackSpeed = 1.1
    P["Slasher Export"]:Destroy() 
    P.Mask:Destroy() 
    P.Machete:Destroy() 
   P.Chainsaw:Destroy()
   local light = Instance.new("PointLight", P.Torso)
light.Color = Color3.fromRGB(255, 0, 0)
light.Range =10
light.Brightness = 5
   local Face = Instance.new("Decal", workspace.Players.Killers.Slasher.Head)
   Face.Name = "NoFace"
   Face.Texture = "rbxassetid://18982667465"
