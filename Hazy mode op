game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()

-- Glitched Eyes
coroutine.wrap(function()
    while true do
        wait(90)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Guestly-V2-2/Doors/refs/heads/main/Glitched%20Eyes"))()
    end
end)()

