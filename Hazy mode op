game:GetService("TextChatService").TextChannels.RBXGeneral:DisplaySystemMessage("<font color='#a46bff'>[Hazy Mode (Recreated)]: Script Succesfully Executed/Loaded.</font>")

local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(189, 97, 255)
