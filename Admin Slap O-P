local function OnSpawnEntity(entityName)
 local sounds = Instance.new("Sound")
sounds.Name = "Success"
sounds.SoundId = "rbxassetid://9115975838"
sounds.Volume = 1
sounds.Parent = game.Workspace
sounds:Play()
wait(3)
sounds:Destroy()
end

local function OnInvalidEntity(entityName)
	local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(255, 0, 0)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("This entity doesn't exist: " .. entityName)
local soundi = Instance.new("Sound")
soundi.Name = "Invalid"
soundi.SoundId = "rbxassetid://83517496538471"
soundi.Volume = 1
soundi.Parent = game.Workspace
soundi:Play()
wait(3)
soundi:Destroy()
end

