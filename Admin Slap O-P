--------------------------------------------------
-- ENTITY GUI
--------------------------------------------------
local entityGui
local entityListFrame
local cooldownRunning = false

local function createEntityGui()
	entityGui = Instance.new("ScreenGui")
	entityGui.Name = "EntitySpawnerGui"
	entityGui.ResetOnSpawn = false
	entityGui.Parent = PlayerGui

	local bg = Instance.new("Frame")
	bg.Size = UDim2.fromScale(0.6,0.4)
	bg.Position = UDim2.fromScale(0.5,0.5)
	bg.AnchorPoint = Vector2.new(0.5,0.5)
	bg.BackgroundColor3 = Color3.fromRGB(60,60,60)
	bg.BorderSizePixel = 0
	bg.Parent = entityGui
	Instance.new("UICorner", bg).CornerRadius = UDim.new(0.05,0)

	-- CLOSE
	local close = Instance.new("TextButton")
	close.Size = UDim2.fromScale(0.08,0.12)
	close.Position = UDim2.fromScale(0.95,0.05)
	close.AnchorPoint = Vector2.new(1,0)
	close.Text = "X"
	close.TextScaled = true
	close.Font = Enum.Font.GothamBold
	close.TextColor3 = Color3.fromRGB(255,255,255)
	close.BackgroundColor3 = Color3.fromRGB(0,0,0)
	close.BorderSizePixel = 0
	close.Parent = bg
	Instance.new("UICorner", close).CornerRadius = UDim.new(0.3,0)

	-- ENTITIES LIST
	local listButton = Instance.new("TextButton")
	listButton.Size = UDim2.fromScale(0.5,0.15)
	listButton.Position = UDim2.fromScale(0.5,0.2)
	listButton.AnchorPoint = Vector2.new(0.5,0.5)
	listButton.Text = "Entities list"
	listButton.TextScaled = true
	listButton.Font = Enum.Font.GothamBold
	listButton.TextColor3 = Color3.fromRGB(255,255,255)
	listButton.BackgroundColor3 = Color3.fromRGB(0,0,0)
	listButton.BorderSizePixel = 0
	listButton.Parent = bg
	Instance.new("UICorner", listButton).CornerRadius = UDim.new(0.2,0)

	-- TEXTBOX
	local entityBox = Instance.new("TextBox")
	entityBox.Size = UDim2.fromScale(0.8,0.18)
	entityBox.Position = UDim2.fromScale(0.5,0.4)
	entityBox.AnchorPoint = Vector2.new(0.5,0.5)
	entityBox.BackgroundColor3 = Color3.fromRGB(0,0,0)
	entityBox.TextColor3 = Color3.fromRGB(255,255,255)
	entityBox.Font = Enum.Font.GothamBold
	entityBox.TextScaled = true
	entityBox.PlaceholderText = "Enter the entity's name to spawn"
	entityBox.ClearTextOnFocus = false
	entityBox.Text = ""
	entityBox.Parent = bg
	Instance.new("UICorner", entityBox).CornerRadius = UDim.new(0.2,0)

	-- SPAWN BUTTON
	local spawnBtn = Instance.new("TextButton")
	spawnBtn.Size = UDim2.fromScale(0.6,0.18)
	spawnBtn.Position = UDim2.fromScale(0.5,0.7)
	spawnBtn.AnchorPoint = Vector2.new(0.5,0.5)
	spawnBtn.Text = "Spawn entity"
	spawnBtn.TextScaled = true
	spawnBtn.Font = Enum.Font.GothamBold
	spawnBtn.TextColor3 = Color3.fromRGB(255,255,255)
	spawnBtn.BackgroundColor3 = Color3.fromRGB(0,0,0)
	spawnBtn.BorderSizePixel = 0
	spawnBtn.Visible = false
	spawnBtn.Parent = bg
	Instance.new("UICorner", spawnBtn).CornerRadius = UDim.new(0.2,0)

	--------------------------------------------------
	-- COOLDOWN DISPLAY
	--------------------------------------------------
	local function startCooldown()
		cooldownRunning = true
		spawnBtn.AutoButtonColor = false

		for i = ENTITY_COOLDOWN, 1, -1 do
			spawnBtn.Text = "Cooldown ("..i..")"
			task.wait(1)
		end

		spawnBtn.Text = "Spawn entity"
		spawnBtn.AutoButtonColor = true
		cooldownRunning = false
	end

	--------------------------------------------------
	-- LOGIC
	--------------------------------------------------
	entityBox:GetPropertyChangedSignal("Text"):Connect(function()
		spawnBtn.Visible = entityBox.Text ~= ""
	end)

	spawnBtn.MouseButton1Click:Connect(function()
	if cooldownRunning then return end

	local entityName = entityBox.Text
	local spawnFunc = EntityList[entityName]

	if spawnFunc then
		-- Entity h·ª£p l·ªá
		pcall(spawnFunc)          -- spawn (kh√¥ng crash script)
		OnSpawnEntity(entityName) -- hook h·ª£p l·ªá
		task.spawn(startCooldown) -- ‚úÖ cooldown LU√îN ch·∫°y
	else
		-- Entity kh√¥ng t·ªìn t·∫°i
		OnInvalidEntity(entityName)
	end
end)

	--------------------------------------------------
	-- ENTITY LIST GUI
	--------------------------------------------------
	local function openEntityList()
	if entityListFrame then entityListFrame:Destroy() end

	entityListFrame = Instance.new("Frame")
	entityListFrame.Size = UDim2.fromScale(0.7,0.7)
	entityListFrame.Position = UDim2.fromScale(0.5,0.55)
	entityListFrame.AnchorPoint = Vector2.new(0.5,0.5)
	entityListFrame.BackgroundColor3 = Color3.fromRGB(70,70,70)
	entityListFrame.BorderSizePixel = 0
	entityListFrame.Parent = bg
	Instance.new("UICorner", entityListFrame).CornerRadius = UDim.new(0.08,0)

	-- ‚ùå CLOSE
	local listClose = Instance.new("TextButton")
	listClose.Size = UDim2.fromScale(0.1, 0.1)
	listClose.Position = UDim2.fromScale(0.98, 0.02)
	listClose.AnchorPoint = Vector2.new(1, 0)
	listClose.Text = "X"
	listClose.TextScaled = true
	listClose.Font = Enum.Font.GothamBold
	listClose.TextColor3 = Color3.fromRGB(255,255,255)
	listClose.BackgroundColor3 = Color3.fromRGB(0,0,0)
	listClose.BorderSizePixel = 0
	listClose.Parent = entityListFrame
	Instance.new("UICorner", listClose).CornerRadius = UDim.new(0.3,0)

	listClose.MouseButton1Click:Connect(function()
		entityListFrame:Destroy()
		entityListFrame = nil
	end)

	-- SCROLL
	local scroll = Instance.new("ScrollingFrame")
	scroll.Size = UDim2.fromScale(0.95,0.85)
	scroll.Position = UDim2.fromScale(0.5,0.55)
	scroll.AnchorPoint = Vector2.new(0.5,0.5)
	scroll.CanvasSize = UDim2.new(0,0,0,0)
	scroll.ScrollBarImageTransparency = 0.3
	scroll.BackgroundTransparency = 1
	scroll.BorderSizePixel = 0
	scroll.Parent = entityListFrame

	local layout = Instance.new("UIListLayout")
	layout.Padding = UDim.new(0,8)
	layout.Parent = scroll

	-- ‚úÖ FIX SCROLL
	layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		scroll.CanvasSize = UDim2.new(0,0,0,layout.AbsoluteContentSize.Y + 10)
	end)

	for name,_ in pairs(EntityList) do
		local cell = Instance.new("TextButton")
		cell.Size = UDim2.new(1,-10,0,40)
		cell.Text = name
		cell.TextScaled = true
		cell.Font = Enum.Font.GothamBold
		cell.TextColor3 = Color3.fromRGB(255,255,255)
		cell.BackgroundColor3 = Color3.fromRGB(0,0,0)
		cell.BorderSizePixel = 0
		cell.Parent = scroll
		Instance.new("UICorner", cell).CornerRadius = UDim.new(0.2,0)

		cell.MouseButton1Click:Connect(function()
			entityBox.Text = name
			spawnBtn.Visible = true
		end)
	end
end
	listButton.MouseButton1Click:Connect(openEntityList)
	close.MouseButton1Click:Connect(function()
		entityGui:Destroy()
		entityGui = nil
	end)
end

--------------------------------------------------
-- BUTTON / TOOL EVENTS
--------------------------------------------------
entitiesButton.MouseButton1Click:Connect(function()
	if entityGui then entityGui:Destroy() end
	createEntityGui()
end)

tool.Equipped:Connect(function()
	entitiesButton.Visible = true
end)

tool.Unequipped:Connect(function()
	entitiesButton.Visible = false
	if entityGui then entityGui:Destroy() entityGui = nil end
end)

game:GetService("Players").LocalPlayer.Backpack["Slap Glove"].TextureId = "rbxassetid://17465852777"

local soundi = Instance.new("Sound")
soundi.Name = "Noti"
soundi.SoundId = "rbxassetid://14879177375"
soundi.Volume = 1
soundi.Parent = game.Workspace
soundi:Play()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(0, 0, 0)
game.Lighting.MainColorCorrection.Contrast = 0.01
wait(0.5)
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()
wait(2)

-- ================= CLEAN CONFIG =================
local UNIQUE_GUI_NAME = "TypewriterSubtitle_CLEAN"

local TextToShow = "You just got an Admin Slap.."
local TypingSpeed = 0.05
local DeleteSpeed = 0.05
local WaitBeforeDelete = 3

local TypingSoundId = "rbxassetid://6042053626"
-- =================================================

local CoreGui = game:GetService("CoreGui")

-- üî• XO√Å GUI C≈® N·∫æU C√íN S√ìT
pcall(function()
	if CoreGui:FindFirstChild(UNIQUE_GUI_NAME) then
		CoreGui[UNIQUE_GUI_NAME]:Destroy()
	end
end)

-- GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = UNIQUE_GUI_NAME
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = CoreGui

local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = ScreenGui
TextLabel.Size = UDim2.new(0.9, 0, 0.18, 0)
TextLabel.Position = UDim2.new(0.05, 0, 0.75)
TextLabel.BackgroundTransparency = 1
TextLabel.Text = ""
TextLabel.TextWrapped = true
TextLabel.Font = Enum.Font.GothamBold
TextLabel.TextScaled = false
TextLabel.TextSize = 30
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.TextStrokeTransparency = 0

-- üîä SOUND (ƒë·∫∑t TRONG GUI ƒë·ªÉ destroy c√πng)
local Sound = Instance.new("Sound")
Sound.SoundId = TypingSoundId
Sound.Volume = 1
Sound.Parent = ScreenGui

-- ===== HI·ªÜN CH·ªÆ =====
for i = 1, #TextToShow do
	TextLabel.Text = TextToShow:sub(1, i)
	Sound:Stop()
	Sound.TimePosition = 0
	Sound:Play()
	task.wait(TypingSpeed)
end

-- ‚è≥ ƒê·ª¢I
task.wait(WaitBeforeDelete)

-- ===== XO√Å CH·ªÆ NG∆Ø·ª¢C =====
for i = #TextToShow, 0, -1 do
	TextLabel.Text = TextToShow:sub(1, i)
	Sound:Stop()
	Sound.TimePosition = 0
	Sound:Play()
	task.wait(DeleteSpeed)
end

-- üßπ CLEANUP TUY·ªÜT ƒê·ªêI
pcall(function()
	TextLabel.Text = ""
	Sound:Stop()
	Sound:Destroy()
	TextLabel:Destroy()
	ScreenGui:Destroy()
end)

-- üíÄ SCRIPT K·∫æT TH√öC ‚Äì KH√îNG C√íN G√å T·ªíN T·∫†I

wait(3)
game.Workspace.Noti:Destroy()
