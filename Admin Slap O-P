--------------------------------------------------
-- CONFIRM GUI
--------------------------------------------------
local confirmGui

local function openConfirm()
    if confirmGui then confirmGui:Destroy() end

    confirmGui = Instance.new("Frame")
    confirmGui.Size = UDim2.fromScale(0.6,0.35)
    confirmGui.Position = UDim2.fromScale(0.5,0.5)
    confirmGui.AnchorPoint = Vector2.new(0.5,0.5)
    confirmGui.BackgroundColor3 = Color3.fromRGB(50,50,50)
    confirmGui.BorderSizePixel = 0
    confirmGui.Parent = gui
    Instance.new("UICorner", confirmGui).CornerRadius = UDim.new(0.05,0)

    -- TEXT
    local title = Instance.new("TextLabel")
    title.Size = UDim2.fromScale(0.9,0.4)
    title.Position = UDim2.fromScale(0.5,0.3)
    title.AnchorPoint = Vector2.new(0.5,0.5)
    title.BackgroundTransparency = 1
    title.TextWrapped = true
    title.TextScaled = true
    title.Font = Enum.Font.GothamBold
    title.TextColor3 = Color3.fromRGB(255,255,255)
    title.Text = "Do you really want to remove all your items?\n(Except Admin Slap)"
    title.Parent = confirmGui

    -- NO BUTTON
    local noBtn = Instance.new("TextButton")
    noBtn.Size = UDim2.fromScale(0.35,0.25)
    noBtn.Position = UDim2.fromScale(0.25,0.75)
    noBtn.AnchorPoint = Vector2.new(0.5,0.5)
    noBtn.Text = "No"
    noBtn.Font = Enum.Font.GothamBold
    noBtn.TextScaled = true
    noBtn.TextColor3 = Color3.fromRGB(255,255,255)
    noBtn.BackgroundColor3 = Color3.fromRGB(200,0,0)
    noBtn.BorderSizePixel = 0
    noBtn.Parent = confirmGui
    Instance.new("UICorner", noBtn).CornerRadius = UDim.new(0.2,0)

    -- YES BUTTON
    local yesBtn = Instance.new("TextButton")
    yesBtn.Size = noBtn.Size
    yesBtn.Position = UDim2.fromScale(0.75,0.75)
    yesBtn.AnchorPoint = Vector2.new(0.5,0.5)
    yesBtn.Text = "Yes"
    yesBtn.Font = Enum.Font.GothamBold
    yesBtn.TextScaled = true
    yesBtn.TextColor3 = Color3.fromRGB(255,255,255)
    yesBtn.BackgroundColor3 = Color3.fromRGB(0,170,0)
    yesBtn.BorderSizePixel = 0
    yesBtn.Parent = confirmGui
    Instance.new("UICorner", yesBtn).CornerRadius = UDim.new(0.2,0)

    --------------------------------------------------
    -- NO LOGIC
    --------------------------------------------------
    noBtn.MouseButton1Click:Connect(function()
        confirmGui:Destroy()
        confirmGui = nil
    end)

    --------------------------------------------------
    -- YES LOGIC (C√ì HOOK)
    --------------------------------------------------
    yesBtn.MouseButton1Click:Connect(function()

        --------------------------------------------------
        -- üîå HOOK SCRIPT PH·ª§ (B·∫†N G·∫ÆN ·ªû ƒê√ÇY)
        local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(255, 92, 92)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("You just removed all your items! (Except Admin Slap)",true)
local sound1 = Instance.new("Sound")
sound1.Name = "Removed"
sound1.SoundId = "rbxassetid://106145041472451"
sound1.Volume = 1
sound1.Parent = game.Workspace
sound1:Play()

        --------------------------------------------------
        -- REMOVE ITEMS LOGIC (GI·ªÆ NGUY√äN)
        --------------------------------------------------
        local function clear(container)
            for _, tool in ipairs(container:GetChildren()) do
                if tool:IsA("Tool") and tool.Name ~= "Slap Glove" then
                    tool:Destroy()
                end
            end
        end

        if player.Character then
            clear(player.Character)
        end
        clear(player.Backpack)

        --------------------------------------------------
        -- ƒê√ìNG CONFIRM GUI
        --------------------------------------------------
        confirmGui:Destroy()
        confirmGui = nil
        wait(5)
        game.Workspace.Removed:Destroy()
    end)
end

removeButton.MouseButton1Click:Connect(openConfirm)

--------------------------------------------------
-- TOOL EVENTS (HI·ªÜN / ·∫®N)
--------------------------------------------------
tool.Equipped:Connect(function()
    removeButton.Visible = true
end)

tool.Unequipped:Connect(function()
    removeButton.Visible = false
    if confirmGui then
        confirmGui:Destroy()
        confirmGui = nil
    end
end)

